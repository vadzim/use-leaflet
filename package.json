{
  "name": "use-leaflet",
  "version": "1.5.1",
  "description": "useLeaflet hook for react-leaflet",
  "license": "MIT",
  "repository": "vadzim/use-leaflet",
  "main": "dist/index.js",
  "author": {
    "name": "Vadzim Zie≈Ñka",
    "email": "v@vadzim.info",
    "url": "https://github.com/vadzim"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage --collectCoverageFrom=src/**/*.js",
    "lint": "eslint .",
    "flow": "flow check",
    "flowinstall": "flow-typed install --ignoreDeps=dev",
    "docs": "documentation --config documentation.yml readme src --section=API",
    "postdocs": "git add README.md",
    "clean": "rimraf dist",
    "flowbuild": "flow-copy-source src dist",
    "prebuild": "npm run docs && npm run clean && npm run flowbuild",
    "build": "babel src -d dist",
    "preversion": "npm run flowinstall && npm run lint && npm test && npm run build",
    "postpublish": "git push origin master --follow-tags",
    "sendcodecov": "codecov --token=$CODECOV_TOKEN"
  },
  "lint-changed": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "useLeaflet",
    "react",
    "hook",
    "hooks",
    "use",
    "leaflet",
    "react-leaflet"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": "16 || 17",
    "react-leaflet": "2",
    "leaflet": "1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "documentation": "^12.1.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "flow-bin": "^0.112.0",
    "flow-copy-source": "^2.0.8",
    "flow-typed": "^2.6.2",
    "jest": "^24.9.0",
    "leaflet": "^1.5.1",
    "lint-changed": "0.0.0-development",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.11.0",
    "react-hooks-testing-library": "^0.6.0",
    "react-leaflet": "^2.5.0",
    "react-test-renderer": "^16.11.0",
    "rimraf": "^3.0.0"
  }
}
